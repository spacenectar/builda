{
  "name": "mock-app",
  "description": "A mock application",
  "builda": {
    "prefab": {
      "name": "prefab-lba-wellspring",
      "version": "0.0.1",
      "path": "https://lba-wellspring.lhowst.apps.gss.gov.uk"
    },
    "blueprints": {
      "component": {
        "location": "prefab"
      },
      "page": {
        "location": "prefab"
      }
    },
    "scripts": {
      "page": {
        "use": "page",
        "outputPath": "front-end/pages"
      },
      "partial": {
        "use": "component",
        "outputPath": "front-end/partials"
      },
      "component": {
        "use": "component",
        "variants": [
          {
            "name": "data-display",
            "outputPath": "front-end/components/data-display"
          },
          {
            "name": "factory",
            "outputPath": "front-end/components/factories"
          }
        ]
      }
    }
  }
}